.clearfix::after{display: block;height: 0;clear: both;content: ''}
page{background:#F5F5F5;}
.store_msg{background:#fff;padding:29rpx 31rpx 42rpx 31rpx;border-bottom:15rpx solid #F5F5F5;overflow:hidden;}
.course_name{color:#333333;font-size:28rpx;font-weight:bold;padding-bottom:18rpx;}
.address_box{padding-top:20rpx;}
.store_address{float:left;width:82%;color:#666;font-size:24rpx;}
.store_distance{float:right;width:18%;color:#666;font-size:24rpx;text-align:right;}
.course_box{padding-top:18rpx;}
.course_key{float:left;color:#666;font-size:24rpx;}
.course_val{float:right;color:#666;font-size:24rpx;}
.price_box{padding-bottom:20rpx;border-bottom:1rpx solid #F0F0F0;}
.store_msg3{background:#fff;padding:29rpx 31rpx 33rpx 31rpx;}
.phone_key{float:left;color:#333;font-size:28rpx;}
.phone_val{float:right;color:#333;font-size:24rpx;}
.agreement{color:#999;font-size:24rpx;padding-top:30rpx;text-align:center;}
.submit_btn{width:100%;height:98rpx;color:#fff;line-height:98rpx;background:#FD5576;position:fixed;bottom:0;border:none;border-radius:0;text-align:center;}
.menb{width:100%;height:100%;background:rgba(0,0,0,0.5);position:fixed;top:0;text-align:center;}
.prompt{width:504rpx;background:#fff;border-radius:10rpx;padding-top:40rpx;margin:400rpx auto;}
.prompt_tit{font-size:32rpx;}
.prompt_num{font-size:28rpx;padding:30rpx 0;}
.prompt_cont{font-size:24rpx;padding-bottom:25rpx;border-bottom:1rpx solid #c8c6c6;}
.prompt_btn{height:90rpx;font-size:30rpx;line-height:80rpx;border: none;border-radius: 30rpx;}